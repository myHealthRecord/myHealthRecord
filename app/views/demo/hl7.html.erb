<h1> Handling HL7 </h1>


<div class="row">
  <div class="col-md-3">
    <%= form_for(:demo, :url => {:action => 'parse'}) do |f| %>
      <%= f.label('HL7 string') %>
      <%= f.text_area(:text, size: "20x20", placeholder: @hl7string.nil? ? '' : @hl7string["demo"]["text"]) %>
      <%= submit_tag("SEND") %>
    <% end %>
  </div>

  <div class="col-md-3">
    <div class="output">
      <p><%= @hl7string.to_s %></p>
    </div>
  </div>

</div>

